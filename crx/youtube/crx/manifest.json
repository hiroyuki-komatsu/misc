{
  "name": "Screenshot and timestamp for YouTube",
  "description" : "Extension to copy the screenshot and timestime of YouTube.",
  "permissions": ["activeTab"],
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
